{
  "name": "ho",
  "version": "1.1.2",
  "description": "Ho ho ho the jolly css/less system",
  "bin": {
    "ho": "lib/ho"
  },
  "scripts": {
    "lint": "eslint ./lib/**/*.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src --out-dir lib",
    "example": "./lib/ho compile -e example/main.less -o example/styles.css -c package.json --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hocss/ho.git"
  },
  "keywords": [
    "ho",
    "css",
    "less",
    "styleguide",
    "compiler",
    "conformance",
    "framework",
    "system"
  ],
  "ho": {
    "autoprefixer-transform": {
      "browsers": [
        "last 4 versions"
      ]
    }
  },
  "author": "Matt Styles",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hocss/ho/issues"
  },
  "homepage": "https://github.com/hocss/ho",
  "dependencies": {
    "babel-core": "^6.2.1",
    "chalk": "^1.1.1",
    "chokidar": "^1.3.0",
    "glob": "^6.0.1",
    "ho-compiler": "^1.0.0",
    "yargs": "^3.30.0"
  },
  "devDependencies": {
    "autoprefixer-transform": "^0.3.1"
  }
}
